{
  "expo": {
    "name": "NutriTrack",
    "slug": "nutritrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2e86de",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.nutritrack"
    },
    "android": {
      "package": "com.yourcompany.nutritrack",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "VIBRATE"
      ]
    },
    "plugins": [
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "e29d18c6-7b0c-424b-b21c-cf7bea33aa84"
      }
    }
  }
}